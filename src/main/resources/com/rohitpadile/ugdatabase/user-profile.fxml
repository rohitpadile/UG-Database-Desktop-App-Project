<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rohitpadile.ugdatabase.ProfileController">

    <children>
        <MenuBar prefHeight="166.0" prefWidth="1280.0" style="-fx-background-color: BlueViolet; visibility: 1000;" VBox.vgrow="NEVER">
            <effect>
                <SepiaTone level="0.23">
                    <input>
                        <Reflection />
                    </input>
                </SepiaTone>
            </effect></MenuBar>
        <AnchorPane prefHeight="694.0" prefWidth="1280.0">
            <children>
                <Label fx:id="currentProfileLabel" layoutX="218.0" layoutY="114.0" prefHeight="78.0" prefWidth="286.0" text="User" textFill="#5711d9">
                    <font>
                        <Font name="Bookshelf Symbol 7" size="50.0" />
                    </font>
                </Label>
                  <Label layoutX="41.0" layoutY="12.0" text="UGConnect" textFill="#0f63b8">
                      <font>
                          <Font size="96.0" />
                      </font>
                  </Label>
            <Button fx:id="addStudentProfile" layoutX="320.0" layoutY="243.0" mnemonicParsing="false" onAction="#addStudent" prefHeight="78.0" prefWidth="312.0" text="Add Student" textFill="#2b72e4">
               <font>
                  <Font name="Bookshelf Symbol 7" size="37.0" />
               </font>
            </Button>
            <Button fx:id="findStudentButton" layoutX="667.0" layoutY="243.0" mnemonicParsing="false" onAction="#findStudent" prefHeight="78.0" prefWidth="312.0" text="Find Student" textFill="#2b72e4">
               <font>
                  <Font name="Bookshelf Symbol 7" size="37.0" />
               </font>
            </Button>
            <Button fx:id="userProfileLogoutButton" layoutX="92.0" layoutY="483.0" mnemonicParsing="false" onAction="#userProfileLogoutButtonHandle" prefHeight="58.0" prefWidth="248.0" text="Logout" textFill="BLUEVIOLET">
               <font>
                  <Font name="Arial Bold" size="32.0" />
               </font>
            </Button>
            </children>
        </AnchorPane>
    </children>
</VBox>
